<!--actual contents of dtt window. to be emebedded within dtt_panel-->

<root>
    <windowclass name="dtt_window">
        <frame>utlitybox</frame>
		<anchored>
			<top offset="20" />
			<left offset="29" />
		</anchored>
        <size>
            <width>500</width>
            <height>250</height>
        </size>
        <sheetdata>
            <windowtitlebar_utilitybox name="title" />
			<stringcontrol name="dtt_title">
                <frame name="windowtitle" />
                <anchored>
                    <top anchor="top" offset="0" />
                    <left anchor="center" offset="0" />
                </anchored>
                <center />
                <label>Hello World</label>
                <default>Hello</default>
            </stringcontrol>
        </sheetdata>
	</windowclass>
</root>