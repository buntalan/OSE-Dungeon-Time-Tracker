<!--actual contents of dtt window. to be emebedded within dtt_panel-->

<root>
    <windowclass name="dtt_window">
        <frame>utilitybox</frame>
        <margin>
            <top>0</top>
            <left>0</left>
            <bottom>0</bottom>
            <right>0</right>
        </margin>
        <minimum>
            <width>200</width>
            <height>100</height>
        </minimum>
        <sheetdata>
            <windowtitlebar_utilitybox name="dttwindowtitle"/>
			<stringcontrol name="dtt_title">
                <anchored>
                    <top anchor="top" offset="0" />
                    <left anchor="center" offset="0" />
                </anchored>
                <center />
                <label>Hello World</label>
                <default>Hello</default>
            </stringcontrol>
        </sheetdata>
	</windowclass>
</root>